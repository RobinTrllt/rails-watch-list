<div class="container">
  <h1>Create a bookmark:</h1>
  <div class="form">
    <%= simple_form_for([@list, @bookmark]) do |f| %>
      <%= f.input :comment %>
      <%= f.association :movie %>
      <%= f.button :submit %>
    <% end %>
  </div>
  <%= link_to("Back to Bookmarks", list_path(@list)) %>
</div>
