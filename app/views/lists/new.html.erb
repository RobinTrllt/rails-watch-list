<div class="container">
  <h1>Create a list:</h1>
  <div class="form">
    <%= simple_form_for(@list) do |f| %>
    <p> <%= f.label :name %>
    <%= f.text_field :name %>
    <p> <%= f.submit %> <p>
<% end %>
  </div>
  <span class="btn" class="back"><%= link_to("Back to Lists", lists_path) %></span>
</div>
